// Return w ifie - flaga w State (1) oraz wartosc reta (2):
//    -1 - memory
//    -2 - ret | continue | break | go
//    -3 - ...

// wywolywanie funkcji - ewaluate vlaue zanim zmienisz env
// czy transExp wgl zmienia env? - nie powinno
// Funkcja zmienia env podczas deklaracji swoj


fun fib(d: Int) : Int {
  if (d == 1 || d == 2) {
    return 1;
  }
  return fib(d-1) + fib(d-2);
}

fun main() : Unit {
  run {
    	return;
	}
  println (fib(8));
}
